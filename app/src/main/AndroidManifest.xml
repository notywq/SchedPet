<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mobidev.carmelalouise.schedpet">

    <application
        android:allowBackup="true"
        android:icon="@drawable/schedpet"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".page.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".page.RegisterPetActivity" />
        <activity android:name=".page.PetProfileActivity" />
        <activity android:name=".page.EditPetActivity" />
        <activity android:name=".page.ViewPetDetailsActivity" />
        <activity android:name=".page.AppointmentsActivity" />
        <activity android:name=".page.VaccinesActivity" />
        <activity android:name=".page.AddVaccineActivity" />
    </application>

</manifest>